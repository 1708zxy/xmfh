/*日历*/
.calendar { width:258px; height:258px; margin:0 auto;}
.rizi .calendar-d{ position:absolute; display:none; left:220px; top:34px; border:1px solid #d9e0e4;  -webkit-border-radius:4px; border-radius:4px; z-index:999; background-color:#fff; }
.rizi .calendar-arrow{ float:none; margin-right:0; margin-left:auto; margin-right:auto; margin-top:10px;}
.calendar-inner { position: relative; z-index: 1; -webkit-perspective: 1000; -moz-perspective: 1000; -ms-perspective: 1000; perspective: 1000;
-ms-transform: perspective(1000px); -moz-transform: perspective(1000px); -moz-transform-style: preserve-3d;-ms-transform-style: preserve-3d;}
.calendar-views { transform-style: preserve-3d;}
.calendar .view { backface-visibility: hidden; position: absolute; top: 0; left: 0; *overflow: hidden; -webkit-transition: .6s; transition: .6s;}
.calendar.calendar-m{ display:none;}
.calendar-d .view-month,
.calendar-m .view-date { transform: rotateY(180deg); visibility: hidden; z-index: 1; display:none;}
.calendar-d .view-date,
.calendar-m .view-month {transform: rotateY(0deg);visibility: visible;z-index: 2;}
.calendar-ct,
.calendar-hd,
.calendar-views .week,
.calendar-views .days {overflow: hidden;}
.calendar-views { width: 100%;}
.calendar .view,
.calendar-display,
.calendar-arrow .prev,
.calendar .date-items li { float: left;}
.calendar-arrow,
.calendar-arrow .next { float: right;}
 
.calendar-arrow .prev{ background:url(../images/pre.png) no-repeat; width:6px; height:9px; display:block; text-indent:1000px; margin-top:5px;}
.calendar-arrow .next{ background:url(../images/next.png) no-repeat; width:6px; height:9px; display:block; text-indent:1000px; margin-top:5px;}

.calendar-hd { padding:0px 0; height: 30px; line-height:24px;}
.calendar-display { font-size:13px; }
.view-month .calendar-hd { padding: 10px;}
.calendar-arrow,
.calendar-display { color: #333; width:100px;}
.calendar li[disabled] {color: #bbb;}
.calendar li.old[disabled],
.calendar li.new[disabled] {color: #eee;}
.calendar-display .m,
.calendar-views .week,
.calendar-views .days .old,
.calendar-views .days .new,
.calendar-display:hover,
.calendar-arrow span:hover { color: #888;}
.calendar-arrow span,
.calendar-views .days li[data-calendar-day],
.calendar-views .view-month li[data-calendar-month] {cursor: pointer;} 
.calendar li[disabled] {cursor: not-allowed;}
.calendar-arrow {width:120px;margin-right: 10px; text-align:center;}
.calendar-arrow span { font-size:13px;}
.calendar ol li {position: relative;float: left;text-align: center;border-radius: 50%;}
.calendar .week li,.calendar .days li {width: 40px;height: 40px;line-height: 40px;}
.calendar .month-items li {width: 70px;height: 70px;line-height: 70px;}
.calendar .days li[data-calendar-day]:hover,
.calendar .view-month li[data-calendar-month]:hover {background: #eee; }
.calendar .calendar-views .now {color: #fff;background:#f74d00!important;  }
.calendar .calendar-views .selected {color: #ff6600;background: #fff0e9!important; }
.calendar .calendar-views .dot {position: absolute;left: 50%;bottom: 4px;margin-left: -2px; width: 6px;height: 6px;background: #f74d00;border-radius: 50%;}
.calendar-views .now .dot {background: #fff;}
.calendar .date-items {width: 300%;margin-left: -100%;}
.calendar-label {display: none;position: absolute;top: 50%;left: 50%;z-index: 2;padding: 5px 10px;line-height: 22px;color: #fff;background: #000;border-radius: 3px;opacity: .7;filter: alpha(opacity=70);  display:none;}
.calendar-label i {display: none;position: absolute;left: 50%;bottom: -12px;width: 0;height: 0;margin-left: -3px;
border: 6px solid transparent;border-top-color: #000;  display:none;}

