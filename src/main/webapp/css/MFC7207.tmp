/*滚动条*/
.ladaycont .uploadbox{ height:320px;}
.uploadbox .div_scroll {width:100%; height:200px; overflow:auto; float:left;}	
.uploadbox .scroll_container {overflow: hidden;position: relative;}
.uploadbox .scroll_absolute { position: absolute;}
.uploadbox .scroll_vertical_bar { position: absolute; top: 0;  right: 0;  width: 8px;  height: 100%; background: #eceff1; margin: 0;  padding: 0; -webkit-border-radius:4px; border-radius:4px;}
.uploadbox .scroll_track {position: relative;background-color: #eceff1;-webkit-border-radius:4px; border-radius:4px; }
.uploadbox .scroll_drag {position: relative;top: 0;left: 0;cursor: pointer;background-color: #8e9196;-webkit-border-radius:4px; border-radius:4px;  }
.uploadbox .scroll_arrow {text-indent: -20000px;display: block;cursor: pointer;background-color: #D6B74B;}
.uploadbox .scroll_vertical_bar .scroll_arrow { height: 0px;}

.ladaycont .uploadbox label{ position:relative;}

